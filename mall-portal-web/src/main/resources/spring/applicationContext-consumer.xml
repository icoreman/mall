<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd 
	http://dubbo.apache.org/schema/dubbo 
	http://dubbo.apache.org/schema/dubbo/dubbo.xsd">

	<dubbo:application name="mall-portal-web" />
	<dubbo:registry address="zookeeper://192.168.1.107:2181" />

	<dubbo:reference id="contentService" check="false" timeout="1000"
		interface="com.xuxx.mall.content.service.ContentService" />
</beans>
